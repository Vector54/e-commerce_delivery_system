<%= form_with model: @shipping_company, url: "/shipping_companies/#{@shipping_company.id}" do |form|%>
  <div>
    <%= form.label :name%>
    <%= form.text_field :name%>
  </div>
  <div>
    <%= form.label :corporate_name%>
    <%= form.text_field :corporate_name%>
  </div>
  <div>
    <%= form.label :cnpj%>
    <%= form.text_field :cnpj%>
  </div>
  <div>
    <%= form.label :billing_adress%>
    <%= form.text_field :billing_adress%>
  </div>
  <div>
    <%= form.label :email_domain%>
    <%= form.text_field :email_domain%>
  </div>
  <%= form.submit%>
<%end%>